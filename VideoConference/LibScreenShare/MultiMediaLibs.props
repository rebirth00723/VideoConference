    <Link>
      <AdditionalLibraryDirectories>$(SolutionDir)$(Platform)\$(Configuration)\;C:\opencv\build\x86\vc12\lib;C:\ffmpeg\lib;C:\SDL\lib\x86;C:\PortAudio\lib;C:\mysqlc\lib;C:\sqlite3\lib;%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
